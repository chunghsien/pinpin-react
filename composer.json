{
    "name": "chunghsien/pinpin_react",
    "description": "Pinpin is web application rapid development tool",
    "type": "project",
    "keywords": [
        "ecommerce",
        "cms",
        "facebook/react",
        "laminas",
        "laminas-mezzio",
        "webpack",
        "middleware",
        "psr",
        "psr-7",
        "psr-11",
        "psr-15",
        "php7"
    ],
    "homepage": "https://github.com/chunghsien/pinpin_react",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laminas": {
            "component-whitelist": [
                "mezzio/mezzio",
                "mezzio/mezzio-helpers",
                "mezzio/mezzio-router",
                "laminas/laminas-httphandlerrunner",
                "mezzio/mezzio-fastroute"
            ]
        }
    },
    "require": {
        "php": "^7.2",
        "firebase/php-jwt": "^5.2",
        "google/recaptcha": "^1.2",
        "gregwar/captcha": "^1.1.7",
        "http-interop/http-middleware": "^0.5.0",
        "intervention/image": "^2.4",
        "laminas/laminas-cache": "^2.9",
        "laminas/laminas-code": "^3.4",
        "laminas/laminas-component-installer": "^2.1.2",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-http": "^2.12",
        "laminas/laminas-hydrator": "^3.1",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-i18n-resources": "^2.6",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-log": "^2.12",
        "laminas/laminas-mail": "^2.10",
        "laminas/laminas-math": "^3.2",
        "laminas/laminas-mime": "^2.7",
        "laminas/laminas-navigation": "^2.9",
        "laminas/laminas-paginator": "^2.8",
        "laminas/laminas-serializer": "^2.9",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stdlib": "^3.2.1",
        "laminas/laminas-stratigility": "^3.2",
        "laminas/laminas-text": "^2.7",
        "laminas/laminas-uri": "^2.7",
        "laminas/laminas-view": "^2.11",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "mezzio/mezzio": "^3.2.1",
        "mezzio/mezzio-csrf": "^1.0",
        "mezzio/mezzio-fastroute": "^3.0.3",
        "mezzio/mezzio-flash": "^1.0",
        "mezzio/mezzio-helpers": "^5.3",
        "mezzio/mezzio-laminasviewrenderer": "^2.2",
        "mezzio/mezzio-session": "^1.3",
        "mezzio/mezzio-session-ext": "^1.7",
        "mezzio/mezzio-twigrenderer": "^2.6",
        "noprotocol/php-mysql-aes-crypt": "^2.0",
        "psr/http-message": "^1.0",
        "sokil/php-isocodes": "^3.0",
        "spatie/image-optimizer": "^1.2",
        "symfony/console": "^4.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "laminas/laminas-development-mode": "^3.2",
        "mezzio/mezzio-tooling": "^1.3",
        "phpspec/prophecy": "^1.10.3",
        "phpunit/phpunit": "^7.5.20 || ^8.5.2 || ^9.0.1",
        "roave/security-advisories": "dev-master",
        "umpirsky/locale-list": "^1.0"

    },
    "autoload": {
        "psr-4": {
            "App\\": "modules/App/src",
            "Chopin\\Console\\": "modules/chunghsien/chopin-console/src",
            "Chopin\\Support\\": "modules/chunghsien/chopin-support/src",
            "Chopin\\Middleware\\": "modules/chunghsien/chopin-middleware/src/",
            "Chopin\\MiddlewareService\\": "modules/chunghsien/chopin-middleware-service/src/",
            "Chopin\\LaminasDb\\": "modules/chunghsien/chopin-laminas-db/src/",
            "Chopin\\LaminasServiceManager\\": "modules/chunghsien/chopin-laminas-service-manager/src/",
            "Chopin\\Documents\\": "modules/chunghsien/chopin-documents/src/",
            "Chopin\\Elfinder\\": "modules/chunghsien/chopin-elfinder/src/",
            "Chopin\\LanguageHasLocale\\": "modules/chunghsien/chopin-language-has-locale/src/",
            "Chopin\\Newsletter\\": "modules/chunghsien/chopin-newsletter/src/",
            "Chopin\\Store\\": "modules/chunghsien/chopin-store/src/",
            "Chopin\\SystemSettings\\": "modules/chunghsien/chopin-system-settings/src/",
            "Chopin\\Users\\": "modules/chunghsien/chopin-users/src/",
            "Chopin\\LaminasPaginator\\": "modules/chunghsien/chopin-laminas-paginator/src",
            "Chopin\\Inputfilter\\": "modules/chunghsien/chopin-inputfilter/src",
            "Chopin\\Validator\\": "modules/chunghsien/chopin-validator/src",
            "Chopin\\HttpMessage\\": "modules/chunghsien/chopin-http-message/src",
            "Chopin\\I18n\\": "modules/chunghsien/chopin-i18n/src",
            "Chopin\\Ecpay\\": "modules/chunghsien/chopin-ecpay/src",
            "Chopin\\Newwebpay\\": "modules/chunghsien/chopin-newwebpay/src",
            "Chopin\\Jwt\\" : "modules/chunghsien/chopin-jwt/src",
            "Chopin\\View\\" : "modules/chunghsien/chopin-view/src"
        },
        "files": [
            "modules/chunghsien/chopin-support/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "mezzio": "mezzio --ansi",
        "clear-config-cache": "php bin/clear-config-cache.php",
        "php-cs-fixer": "./vendor/bin/php-cs-fixer fix",
        "serve": "php -S 0.0.0.0:8080 -t public/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
